def GetLSProData(page_data,Cookie_Jar,m):
    import re
    import requests
    mozhdr = {'User-Agent': 'Mozilla/5.0 (Windows NT 6.3; WOW64; rv:55.0) Gecko/20100101 Firefox/55.0', 'Referer': 'https://www.elitegol.fun/'}
    url = '$doregex[id2]'
    html = requests.get(url, headers=mozhdr, verify=False).text
    edata = re.findall('javascript.*?(eval\(function\(p,a,c,k,e,d.*)', html)[0]
    epage = jsunpack.unpack(edata).replace('\\', '')
    fin = re.findall('file:"(h.*?)"', epage)[0]
    return fin
]]></expres>
