from urlresolver.plugins.lib import jsunpack
def GetLSProData(page_data,Cookie_Jar,m):
  import re
  import requests
  mozhdr = {'User-Agent': 'Mozilla/5.0 (Windows NT 10.0; Win64; x64; rv:66.0) Gecko/20100101 Firefox/66.0', 'Referer': 'http://www.seriespapaya.com/enlace/1748549658/'}
  url = 'http://gamovideo.com/tdq1pms2oust'
  html = requests.get(url, headers=mozhdr, verify=False).text
  refvar1,refvar2 = re.findall('(?s)vplayer...img src=.(http://.*?\/).*?mp4.(.*?)\|', html)[0]
  return '{}{}/v.mp4|{}'.format(refvar1, refvar2, mozhdr['User-Agent'])
]]></expres>
